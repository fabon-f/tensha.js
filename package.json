{
  "name": "tensha",
  "version": "0.1.1",
  "description": "Transliteration library from foreign languages to Japanese",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./languages": {
      "types": "./lib/languages/index.d.ts",
      "default": "./lib/languages/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">=14.15"
  },
  "scripts": {
    "test": "ava src",
    "build": "rimraf lib '*.tsbuildinfo' && tsc",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabon-f/tensha.js.git"
  },
  "keywords": [
    "transliterate", "transliteration",
    "russian", "japanese"
  ],
  "author": "fabon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabon-f/tensha.js/issues"
  },
  "homepage": "https://github.com/fabon-f/tensha.js#readme",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ava": "^4.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
